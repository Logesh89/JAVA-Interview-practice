import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int t=0;
	    Scanner sc=new Scanner(System.in);
	    int f=sc.nextInt();
	    int s=sc.nextInt();
	    for(int i=2;i<=8;i++){
	        t=f+s;
	        f=s;
	        s=t;
	    }
	    System.out.print(s);
		
		
	}
}
