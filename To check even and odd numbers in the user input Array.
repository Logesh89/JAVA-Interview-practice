import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    Scanner sc = new Scanner (System.in);
	    int a=sc.nextInt();
	    int arr[]= new int[a];
	    for(int i=0;i<a;i++)
	    {
	        arr[i]=sc.nextInt();
	   }
	   System.out.println("Even numbers are: ");
	   for(int i=0;i<a;i++)
	   {
	       if(arr[i]%2==0)
	       {
	           System.out.println(arr[i]);
	           
	       }
	      
	   }
	   System.out.println("Odd numbers are: ");
	   for(int i=0;i<a;i++)
	   {
	       if(arr[i]%2!=0)
	       {
	           System.out.println(arr[i]);
	           
	       }
	      
	   }
	        
	    
	}
}
