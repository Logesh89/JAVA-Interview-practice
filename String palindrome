import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    Scanner sc=new Scanner(System.in);
	    String a=sc.nextLine();
	    String org_a=a;
	    String rev="";
	    int len=a.length();
	    for(int i=len-1;i>=0;i--){
	        rev=rev+a.charAt(i);
	    }
		//System.out.println("After reverse  are: "+rev);
		if(org_a.equals(rev))
		{
		    System.out.println(org_a+"\nPallindrome");
		}
		else
		{
		    System.out.println(org_a+"\nNot Pallindrome");
		}
	}
}
