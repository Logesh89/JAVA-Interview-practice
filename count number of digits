//count number of digits
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    int i,j;
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int count=0;
		for( i=n;i>0;i/=10){  //->  /=10 Eliminates the last digit
		    
		    count++;
		}
		System.out.println(count);
	}
}


input:
1234

output:
4
