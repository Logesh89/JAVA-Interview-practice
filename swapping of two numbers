import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    int a=sc.nextInt();
	    int b=sc.nextInt();
		System.out.println("Before swapping are: "+a+" "+b);
		int t=a;
		a=b;
		b=t;
		System.out.println("After swapping  are: "+a+" "+b);
	}
}
