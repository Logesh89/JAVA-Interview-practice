import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Random rand=new Random();          //for random integers
		int a=rand.nextInt(100);
	    System.out.println(a);
	    
	    double b=rand.nextDouble();    // for range 0.1 and less than 1.0
	    System.out.println(b);
	    System.out.println(Math.random()); // to generate random decimal numbers
	    
	    //String c= RandomStringUtils.randomNumeric(5);  //need to install the package to generate  http://commons.apache.org/ 
	   // System.out.println(c);   //Returns the 5 digit numbers
	   
	   //String d= RandomStringUtils.randomAlphabetic(5);
	   //System.out.println(d);
	}
}
